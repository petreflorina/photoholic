{% extends '@App/base.html.twig' %}

{% block title %} Add picture {% endblock %}

{%  block body %}
    <div class="form">
        <form enctype="multipart/form-data" action="{{ path('createPicture') }}" method="post">

            <input type="text" name="title" title="Title" >
            <input type="text" name="description" title="Description">
            <input type="text" name="name" title="name">
            <input type="file" name="picture">
            <input type="submit" value="submit">

        </form>
    </div>
{% endblock body %}